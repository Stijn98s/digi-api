{
  "name": "digi-api",
  "version": "0.0.1",
  "description": "Backend for digicatch app",
  "author": "Geordi Trapman",
  "license": "MIT",
  "repository": "https://project.stijn98s.nl/project/digi-api.git",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "build:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ops": "jest --coverage --config ./jest.ci.config.js --no-cache",
    "test:cov": "jest --coverage --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand  --config ./test/jest-e2e.json",
    "test:e2e:cover": "jest --runInBand  --config ./test/jest-e2e.json --coverage",
    "test:ops:cover": "jest --runInBand  --config ./test/jest-e2e.ci.json --coverage",
    "start": " NODE_ENV=production node ./dist/server",
    "webpack": "webpack --config webpack.config.js",
    "docker:run": "docker-compose -f ./ci-conf/docker-compose.localdev.yml up --force-recreate",
    "docker:stop": "docker stop $(docker ps -aq)"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.5",
    "@nestjs/core": "^6.0.5",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/mongoose": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.5",
    "@nestjs/swagger": "^3.0.2",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.1",
    "@types/mocha": "^5.2.6",
    "@types/mongoose": "^5.3.25",
    "bcryptjs": "^2.4.3",
    "class-transformer": "latest",
    "class-validator": "latest",
    "dotenv": "^6.2.0",
    "email-validator": "^2.0.4",
    "joi": "latest",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "nestjs-typegoose": "^5.1.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "latest",
    "prom-client": "^11.3.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "swagger-ui-express": "^4.0.2",
    "typegoose": "^5.6.0",
    "typescript": "^3.4.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.5",
    "@types/cors": "^2.8.4",
    "@types/joi": "^14.3.2",
    "@types/node": "^11.13.2",
    "@types/passport-facebook": "^2.1.8",
    "@types/reflect-metadata": "^0.1.0",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "jest": "^24.7.1",
    "jest-simple-summary": "^1.0.1",
    "mongodb-memory-server": "latest",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.15.0",
    "typemoq": "^2.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.6.0"
  }
}
